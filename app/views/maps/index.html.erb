<!DOCTYPE html>
<html>
  <head>
    <!-- This stylesheet contains specific styles for displaying the map
         on this page. Replace it with your own styles as described in the
         documentation:
         https://developers.google.com/maps/documentation/javascript/tutorial -->

             <style>
                #map {
                 height: 100vh;
                 width: 100%;
                }
             </style>
  </head>
  <body>
    <div id="map"></div>
    <script>
      function initMap() {
        var myLatLng = {lat: 25.8002131, lng: -80.2043848};
        var myLatLng2 = {lat: 25.79961999999999, lng: -80.19828000000001};

        // Create a map object and specify the DOM element for display.
        var map = new google.maps.Map(document.getElementById('map'), {
          center: myLatLng,
          scrollwheel: false,
          zoom: 16
        });

        // Create a marker and set its position.
        var marker = new google.maps.Marker({
          map: map,
          position: myLatLng,
          title: 'Hello World!'
        });
        var marker2 = new google.maps.Marker({
          map: map,
          position: myLatLng2,
          title: 'Test'
        });
      }

    </script>
    <script src="https://maps.googleapis.com/maps/api/js?key=AIzaSyBJN7ZYLilbKwsce1e5gOVik9x1NUGt7yg&callback=initMap"
        async defer></script>
  </body>
</html>
